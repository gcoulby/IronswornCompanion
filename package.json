    "dataforged": "github:rsek/dataforged#v1.1.1",
    "font-awesome": "^4.7.0",
    "frontend-collective-react-dnd-scrollzone": "^1.0.2",
    "gapi-script": "^1.2.0",
    "gh-pages": "^3.1.0",
    "html-react-parser": "^1.2.4",
    "htmlparser2": "^3.10.1",
    "http-server": "^14.1.0",
    "iconify": "^1.3.0",
    "jquery": "^3.5.1",
    "jszip": "^3.6.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "patch-package": "^6.4.7",
    "prosemirror-transform": "1.2.5",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.0",
    "react-contenteditable": "^3.3.5",
    "react-dom": "^17.0.2",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-sortable-tree": "^1.8.1",
    "remark-gfm": "^1.0.0",
