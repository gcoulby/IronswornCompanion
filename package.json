    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "postinstall": "patch-package",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "npm run prebuild && NODE_OPTIONS=--openssl-legacy-provider PORT=3010 PUBLIC_URL=http://localhost:3010/ react-scripts start",
    "prebuild": "cp ./README.md ./src/README.md && cp ./LICENSE ./src/LICENSE && if [ -n \"${GAME_RULES}\" ]; then cp src/css/src/scss/main-$GAME_RULES.scss src/css/src/scss/main.scss; fi",
    "build-css": "sass src/css/src/scss/main.scss src/css/css-compiled/main.css",
    "watch-css": "npm run build-css && sass src/css/src/scss/main.scss src/css/css-compiled/main.css --watch",
    "build": "npm run build-css && NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "webpack-start": "webpack-dev-server --mode development --open --hot",
    "webpack-build": "webpack --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "git checkout -- .;git clean -dfx;git status",
    "setup": "npm i --legacy-peer-deps",
    "http-serve": "http-server -p $PORT build"
  },
  "engines": {
    "node": "18.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.51.0"
  }
}
